<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dam.proyecto.jamarcas.usuario" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Principal_Activity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUPActivity"></activity>
        <activity android:name=".Usuario"></activity>
        <activity android:name=".AlarmaDetalle">
            <intent-filter>
                <action android:name="com.dam.proyecto.jamarcas.AlarmaDetalle" />
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".MostrarNotificacion">
            <intent-filter>
                <action android:name="com.dam.proyecto.jamarcas.MostrarNotificacion" />
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".Medicamento"></activity>
    </application>

</manifest>
